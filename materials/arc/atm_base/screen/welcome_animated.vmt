"UnlitGeneric"
{
	"$basetexture" "arc/atm_base/screen/welcome_animated"
	"$vertexcolor" 		1 
	"$vertexalpha" 		1 
	"$nolod" 			1
	"$translucent" 1
	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" "1"
		}
	}
}